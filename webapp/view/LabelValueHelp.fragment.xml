<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
		noDataText="No Labels Found"
		title="标签选择弹窗"
		search=".labelHandleSearch"
		confirm=".labelHandleValueHelpClose"
		cancel=".labelHandleValueHelpClose"
		showClearButton="false"
		items="{
			path : 'LabelModel>/',
			sorter : {
				path : 'Zbqbh',
				descending : false
			}
		}">
		<ColumnListItem >
			<cells>
				<Text text="{LabelModel>Zbqbh}" />
				<Text text="{LabelModel>Zbqgn}" />
				<Text text="{LabelModel>Zbqmc}" />
				<Text text="{LabelModel>Zsfsp}" />
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="12em">
				<header>
					<Text text="标签编号" />
				</header>
			</Column>
			<Column width="12em">
				<header>
					<Text text="标签功能" />
				</header>
			</Column>
			<Column width="12em">
				<header>
					<Text text="标签名称" />
				</header>
			</Column>
			<Column width="12em">
				<header>
					<Text text="是否双排打印" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>